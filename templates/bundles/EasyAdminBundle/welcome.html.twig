                <div class="card-body py-1 d-flex justify-content-between align-items-center h-100 py-3">
                    <h5 class=""><b>{{results.workData.count}}</b></h5>
                    <a href="{{path('easyadmin',{crudAction: constant('EasyCorp\\Bundle\\EasyAdminBundle\\Config\\Action::INDEX'),crudControllerFqcn: results.workData.url })}}" class="btn btn-dark"> <i class="fa fa-link"></i> </a>
