{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1.0",
        "ext-bcmath": "*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "cravler/maxmind-geoip-bundle": "^2.0",
        "damienharper/auditor-bundle": "5.x-dev",
        "doctrine/dbal": "^3.1",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.10",
        "endroid/qr-code": "^5.0",
        "giggsey/libphonenumber-for-php": "^8.12",
        "guzzlehttp/guzzle": "~6.0",
        "jms/serializer-bundle": "^4.0",
        "league/oauth2-server-bundle": "^0.5",
        "limenius/liform": "dev-master",
        "limenius/liform-bundle": "dev-master",
        "maximosojo/baseadmin-bundle": "dev-main",
        "maximosojo/tools-bundle": "5.x-dev",
        "nelmio/cors-bundle": "^2.2",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.5",
        "pragmarx/google2fa": "8.x-dev",
        "ramsey/uuid-doctrine": "^1.8",
        "rollerworks/password-strength-bundle": "^2.2",
        "stof/doctrine-extensions-bundle": "^1.7",
        "symfony/asset": "^6.2",
        "symfony/console": "^6.2",
        "symfony/dotenv": "^6.2",
        "symfony/flex": "^2.0",
        "symfony/framework-bundle": "^6.2",
        "symfony/lock": "^6.2",
        "symfony/mailer": "^6.2",
        "symfony/monolog-bundle": "^3.7",
        "symfony/property-access": "^6.2",
        "symfony/property-info": "^6.2",
        "symfony/proxy-manager-bridge": "^6.2",
        "symfony/runtime": "^6.2",
        "symfony/security-bundle": "6.2.*",
        "symfony/serializer": "^6.2",
        "symfony/uid": "^6.2",
        "symfony/validator": "^6.2",
        "symfony/webpack-encore-bundle": "^1.17",
        "symfony/yaml": "^6.2",
        "tetranz/select2entity-bundle": "^3.1",
        "vich/uploader-bundle": "*"
    },
    "require-dev": {
        "behat/behat": "^3.10",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "friends-of-behat/mink-browserkit-driver": "^1.6",
        "friends-of-behat/mink-extension": "^2.7",
        "friends-of-behat/symfony-extension": "^2.3",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^0.15.10",
        "symfony/browser-kit": "^6.2",
        "symfony/css-selector": "^6.2",
        "symfony/debug-bundle": "^6.2",
        "symfony/maker-bundle": "^1.36",
        "symfony/phpunit-bridge": "^6.2",
        "symfony/stopwatch": "^6.2",
        "symfony/web-profiler-bundle": "^6.2"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true,
            "phpstan/extension-installer": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "maximosojo/tools-bundle": "source",
            "maximosojo/baseadmin-bundle": "source",
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "cravler:maxmind:geoip-update": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.2.*"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/maximosojo/LiformBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/maximosojo/Liform"
        },
        {
            "type": "vcs",
            "url": "https://github.com/maximosojo/baseadmin-bundle"
        }
    ]
}
